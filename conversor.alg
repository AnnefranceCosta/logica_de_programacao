algoritmo "Conversor"
// Função : Converter angulo
// Autor :  Annefrance Costa
// Data : 30/08/2022
// Seção de Declarações 
var
   angulo, seno: Real
inicio
      Escreva ("Informe o angulo: ")
      Leia(angulo)
      seno <- Sen(GraupRad(angulo))
      Escreva("O seno de ", angulo, " é igual a ", seno)
fimalgoritmo
